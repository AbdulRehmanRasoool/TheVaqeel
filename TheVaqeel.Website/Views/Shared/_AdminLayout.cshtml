@{
    var userName = User.Identity.Name;
    var Id = ViewBag.UserId;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    @Styles.Render("~/Content/Admincss")
    @Scripts.Render("~/bundles/AdminScripts")
    @RenderSection("scripts", required: false)
    <link href="//cdn.datatables.net/1.13.4/css/jquery.dataTables.min.css" rel="stylesheet" />
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="//cdn.datatables.net/1.13.4/js/jquery.dataTables.min.js"></script>
</head>
<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">


        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>

            </ul>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Navbar Search -->
                <li class="nav-item">
                    @Html.ActionLink("GO TO WEBSITE", "Index", "Home", null, new { @class = "nav-link" })
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-light-primary elevation-4">
            <!-- Brand Logo -->
            <a href="@Url.Action("Index","Admin")" class="brand-link">
                <img src="~/Content/img/AdminImage/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">@userName</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item">
                            <a href="@Url.Action("Index","Admin")" class="nav-link">
                                <i class="nav-icon fas fa-tachometer-alt"></i>
                                <p>
                                    Dashboard
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="@Url.Action("ContactUs","Admin")" class="nav-link">
                                <i class="nav-icon fas fa-phone-office"></i>
                                <p>
                                    Contact Queries
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-users"></i>
                                <p>
                                    Lawyers / Advocate
                                    <i class="fas fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("LawyersList","Admin")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>All Lawyers</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("LawyerVerificationList","Admin")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Lawyers Verification</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-user"></i>
                                <p>
                                    Users
                                    <i class="fas fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("UsersList","Admin")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>All Users</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-calendar-alt"></i>
                                <p>
                                    Appointments
                                    <i class="fas fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="pages/forms/general.html" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Manage</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-copy"></i>
                                <p>
                                    Practice Areas
                                    <i class="fas fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("PracticeAreaList","Admin")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Manage</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-gavel"></i>
                                <p>
                                    Lawyer Courts
                                    <i class="fas fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("LawyerLocationList","Admin")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Manage</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-folder"></i>
                                <p>
                                    Content
                                    <i class="fas fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("HomeNavbar","Admin")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Navbar</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("HomeSlider","Admin")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Slider</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("LawyerServicesList","Admin")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Lawyer Services</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("FaqsList","Admin")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>FAQs</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon fas fa-user-cog"></i>
                                <p>
                                    Account Settings
                                    <i class="fas fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="@Url.Action("AdminProfile","Admin",new {id = Id})" class="nav-link" data-id="@ViewBag.UserId" id="AdminProfileBtn">
                                        <i class="far fa-circle nav-icon"></i>
                                        Profile
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="@Url.Action("ChangePassword","Manage")" class="nav-link">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Change Password</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.main-siderbar-container -->
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>Copyright <a href="@Url.Action("Index","Home")">The Vaqeel</a>.</strong>
            Control Panel For Adminstrator
            <div class="float-right d-none d-sm-inline-block">
                All Rights Reserved
            </div>
        </footer>

        <!-- "~/bundles/AdminScripts"ontrol Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->

    </div>
    @section scripts{
        <script>
            $(document).ready(function () {
                $("#AdminProfileBtn").click(function () {
                    alert("J");
                });
            });
        </script>
    }

</body>


</html>
